journal = {US Department of Health and Human Services},
number = {July},
pages = {1--27},
pmid = {17034633},
title = {{FDA guideline: Estimating the Maximum Safe Starting Dose in Initial Clinical Trials for Therapeutics in Adult Healthy Volunteers}},
