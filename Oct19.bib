journal = {Annals of Oncology},
number = {suppl{\_}5},
pages = {418--419},
title = {{PK and PD of CEA-IL2v in first-in-human phase I study}},
url = {http://academic.oup.com/annonc/article/doi/10.1093/annonc/mdx376/4109287/1144OPhase-III-randomized-controlled-trial-of},
