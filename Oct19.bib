doi = {10.1093/toxsci/kfs316},
file = {:C$\backslash$:/Users/UNIV/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Brinkmann et al. - 2013 - Metabolically competent human skin models Activation and genotoxicity of benzoapyrene.pdf:pdf},
issn = {10966080},
journal = {Toxicological Sciences},
keywords = {Benzo[a]pyrene,Comet assay,Human skin,Metabolism,Primary normal human epidermal keratinocytes,Skin models},
number = {2},
pages = {351--359},
title = {{Metabolically competent human skin models: Activation and genotoxicity of benzo[a]pyrene}},
