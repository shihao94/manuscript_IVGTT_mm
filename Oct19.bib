journal = {Gastroenterology},
number = {6},
pages = {1326--30},
pmid = {4054524},
title = {{Effect of meal volume and energy density on the gastric emptying of carbohydrates.}},
url = {http://dx.doi.org/10.1016/0016-5085(85)90650-X},
volume = {89},
