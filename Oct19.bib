issn = {10916490},
journal = {PNAS},
keywords = {Antibody,Charge,Engineering,FcRn,Pharmacokinetics},
number = {19},
pages = {5997--6002},
title = {{Charge-mediated influence of the antibody variable domain on FcRn-dependent pharmacokinetics}},
