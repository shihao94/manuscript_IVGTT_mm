journal = {Pharmaceutical Research},
keywords = {Dynamic light scattering,Hydrodynamic size,Protein aggregation,Protein characterization,Taylor dispersion analysis},
number = {9},
pages = {2302--2310},
title = {{Taylor dispersion analysis compared to dynamic light scattering for the size analysis of therapeutic peptides and proteins and their aggregates}},
volume = {28},
year = {2011}
}
