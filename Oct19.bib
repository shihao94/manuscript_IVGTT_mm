issn = {0002-9513},
journal = {The American journal of physiology},
number = {1 Pt 2},
pages = {H358--63},
pmid = {8769772},
title = {{Flow velocity of single lymphatic capillaries in human skin.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/8769772},
volume = {270},
