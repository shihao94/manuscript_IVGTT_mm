doi = {10.1186/1476-7120-4-45},
file = {:C$\backslash$:/Users/UNIV/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Lavine - 2006 - Effect of changes in contractility on the index of myocardial performance in the dysfunctional left ventricle.pdf:pdf},
