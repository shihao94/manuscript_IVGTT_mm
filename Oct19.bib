keywords = {Human anti-TNF$\alpha$ monoclonal antibody,absorption,bioavailability,biologic,golimumab,phar-macokinetics,subcutaneous},
pages = {276--284},
title = {{Subcutaneous Bioavailability of Golimumab at 3 Different Injection Sites in Healthy Subjects}},
