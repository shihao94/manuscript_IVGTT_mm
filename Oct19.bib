journal = {Histochemistry and Cell Biology},
keywords = {Gastrointestinal tract,Hexose transporter,In situ hybridization,Mouse,Real-time PCR},
number = {2},
pages = {183--194},
pmid = {21274556},
title = {{Comparative expression of hexose transporters (SGLT1, GLUT1, GLUT2 and GLUT5) throughout the mouse gastrointestinal tract}},
