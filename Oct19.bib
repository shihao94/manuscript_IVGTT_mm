doi = {10.1111/jcpt.12101},
file = {:C$\backslash$:/Users/UNIV/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Shida et al. - 2014 - The pharmacokinetics and safety profiles of belimumab after single subcutaneous and intravenous doses in healthy J.pdf:pdf},
issn = {02694727},
journal = {Journal of Clinical Pharmacy and Therapeutics},
keywords = {Japanese,belimumab,healthy,pharmacokinetics,safety},
month = {feb},
number = {1},
pages = {97--101},
title = {{The pharmacokinetics and safety profiles of belimumab after single subcutaneous and intravenous doses in healthy Japanese volunteers}},
