file = {:C$\backslash$:/Users/UNIV/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Del Prato et al. - 1997 - Studies on the mass action effect of glucose in NIDDM and IDDM Evidence for glucose resistance.pdf:pdf},
