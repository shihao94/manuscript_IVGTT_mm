file = {:C$\backslash$:/Users/UNIV/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Medema et al. - 1996 - Assessment of the dose-response relationship of Campylobacter jejuni.pdf:pdf},
issn = {01681605},
journal = {International Journal of Food Microbiology},
keywords = {Campylobacter jejuni,Dose-response,Infection,Risk},
number = {1-2},
