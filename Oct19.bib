journal = {European Journal of Clinical Pharmacology},
keywords = {Anti-interleukin-23,First-in-human,Guselkumab,Monoclonal antibody,Pharmacokinetics,Safety},
pages = {1303--1310},
title = {{First-in-human study to assess guselkumab (anti-IL-23 mAb) pharmacokinetics/safety in healthy subjects and patients with moderate-to-severe psoriasis}},
