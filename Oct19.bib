keywords = {Absorption,Antibody-drug conjugate (ADC),Biologics,Biotherapeutics,Distribution,Excretion,Imaging,Labeling,Metabolism,Monoclonal antibody (mAb),Neonatal Fc receptor (FcRn),Pharmacokinetics,Subcutaneous bioavailability,review},
