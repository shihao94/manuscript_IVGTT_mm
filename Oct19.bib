journal = {J Pharmacokinet Pharmacodyn},
keywords = {Antibody,Fc,Fc receptor {\textperiodcentered},IgG {\textperiodcentered},Neonatal,PBPK {\textperiodcentered},Pharmacokinetics {\textperiodcentered},Rn {\textperiodcentered}},
pages = {687--709},
