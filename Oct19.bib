issn = {0022-3166},
journal = {Journal of Nutrition},
keywords = {food intake,ghrelin,glucagon-like peptide 1,leptin,vagal afferent neurons},
pages = {jn.114.206029--},
pmid = {25833771},
title = {{Glucagon-Like Peptide 1 Interacts with Ghrelin and Leptin to Regulate Glucose Metabolism and Food Intake through Vagal Afferent Neuron Signaling}},
url = {http://jn.nutrition.org/content/early/2015/02/04/jn.114.206029.abstract?papetoc},
