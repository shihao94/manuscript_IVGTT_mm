issn = {01683659},
journal = {Journal of Controlled Release},
keywords = {Adenovirus,Hyaluronan,Hyaluronidase,Interferon,Monoclonal antibodies,PH20,Subcutaneous bioavailability,Subcutaneous drug delivery},
