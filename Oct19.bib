file = {:C$\backslash$:/Users/UNIV/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Oosaka, Tokuda, Furukawa - 2014 - Intra-gastric triacetin alters upper gastrointestinal motility in conscious dogs.pdf:pdf},
