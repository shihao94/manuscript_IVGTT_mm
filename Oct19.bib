journal = {Proc Natl Acad Sci U S A},
keywords = {*Immune Tolerance,*Lymphocyte Activation,*Signal Transduction,Animals,Antigen-Presenting Cells/immunology,Flow Cytometry,Lymphopenia/immunology,Mice,Mice, Inbred BALB C,Receptors, Antigen, T-Cell/*metabolism},
number = {42},
pages = {18085--18090},
pmid = {20921406},
title = {{Duration of antigen receptor signaling determines T-cell tolerance or activation}},
url = {http://www.ncbi.nlm.nih.gov/entrez/query.fcgi?cmd=Retrieve{\&}db=PubMed{\&}dopt=Citation{\&}list{\_}uids=20921406{\%}5Cnhttp://www.ncbi.nlm.nih.gov/pmc/articles/PMC2964228/pdf/pnas.201010560.pdf},
