issn = {0022-3565},
journal = {Journal of Pharmacology and Experimental Therapeutics},
number = {2},
pages = {246--258},
pmid = {28275201},
title = {{Physiologically Based Pharmacokinetic Model of All- {\textless}i{\textgreater}trans{\textless}/i{\textgreater} -Retinoic Acid with Application to Cancer Populations and Drug Interactions}},
