doi = {10.1002/cpdd.60},
file = {:C$\backslash$:/Users/UNIV/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Ortega, Yancey, Cozens - 2014 - Pharmacokinetics and absolute bioavailability of mepolizumab following administration at subcutaneous an.pdf:pdf},
issn = {2160763X},
journal = {Clinical Pharmacology in Drug Development},
keywords = {Absorption,Bioavailability,Biologic,Human anti-IL5 monoclonal antibody,Intramuscular,Intravenous,Mepolizumab,Pharmacokinetics,Subcutaneous},
month = {jan},
