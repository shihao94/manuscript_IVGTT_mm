journal = {Surgery for Obesity and Related Diseases},
keywords = {Bariatric surgery,Gastric emptying,Incretins,Obesity,Oral glucose tolerance test},
number = {7},
pages = {1320--1327},
pmid = {27012873},
publisher = {Elsevier},
title = {{Incretin effects, gastric emptying and insulin responses to low oral glucose loads in patients after gastric bypass and lean and obese controls}},
