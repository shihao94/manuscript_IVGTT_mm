doi = {10.1186/1471-2407-10-255},
file = {:C$\backslash$:/Users/UNIV/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Lee, Tannock - 2010 - The distribution of the therapeutic monoclonal antibodies cetuximab and trastuzumab within solid tumors.pdf:pdf},
