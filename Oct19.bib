doi = {10.1002/prp2.380},
file = {:C$\backslash$:/Users/UNIV/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Hillson et al. - 2018 - Pharmacokinetic equivalence, comparable safety, and immunogenicity of an adalimumab biosimilar product (M923) to.pdf:pdf},
