journal = {Scandinavian Journal of Gastroenterology},
keywords = {Breath test,Gastric emptying,Paracetamol absorption test,Scintigraphy},
number = {10},
pages = {1182--1186},
pmid = {17852845},
title = {{Gastric emptying: A comparison of three methods}},
volume = {42},
