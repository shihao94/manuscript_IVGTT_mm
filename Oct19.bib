isbn = {1550-7416 (Electronic)$\backslash$r1550-7416 (Linking)},
issn = {1550-7416},
journal = {The AAPS Journal},
keywords = {amgen inc,california,ligand-binding assays,monoclonal antibody biotherapeutics,one amgen center drive,pd,pk,target biomarkers,thousand oaks,total and free assays},
number = {1},
pages = {99--110},
