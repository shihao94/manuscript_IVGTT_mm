number = {6},
pages = {297--301},
title = {{A randomized study of the relative pharmacokinetics, pharmacodynamics, and safety of alirocumab, a fully human monoclonal antibody to PCSK9, after single subcutaneous administration at three different injection sites in healthy subjects}},
