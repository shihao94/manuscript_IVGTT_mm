file = {:C$\backslash$:/Users/UNIV/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Center for Drug Evaluation and Research - 2005 - FDA guideline Estimating the Maximum Safe Starting Dose in Initial Clinical Trials for.pdf:pdf},
