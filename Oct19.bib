file = {:C$\backslash$:/Users/UNIV/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Tham et al. - 2017 - Modeling Pharmacokinetic Profiles of Insulin Regimens to Enhance Understanding of Subcutaneous Insulin Regimens.pdf:pdf},
issn = {15524604},
journal = {Journal of Clinical Pharmacology},
keywords = {Shiny,diabetes,insulin,modeling,pharmacokinetics,simulation},
number = {9},
pages = {1126--1137},
title = {{Modeling Pharmacokinetic Profiles of Insulin Regimens to Enhance Understanding of Subcutaneous Insulin Regimens}},
volume = {57},
year = {2017}
}
@article{Iwahashi2014,
