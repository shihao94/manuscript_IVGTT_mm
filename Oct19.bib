file = {:C$\backslash$:/Users/UNIV/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Langguth et al. - 1994 - Variable gastric emptying and discontinuities in drug absorption profiles Dependence of rates and extent of cim.pdf:pdf},
isbn = {0142-2782 (Print)},
issn = {1099081X},
journal = {Biopharmaceutics {\&} Drug Disposition},
