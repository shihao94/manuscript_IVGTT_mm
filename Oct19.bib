doi = {10.2165/11535960-000000000-00000},
file = {:C$\backslash$:/Users/UNIV/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Dirks, Meibohm - 2010 - Population pharmacokinetics of therapeutic monoclonal antibodies.pdf:pdf},
issn = {03125963},
journal = {Clinical Pharmacokinetics},
keywords = {Drug-interactions,Monoclonal-antibodies,Pharmacodynamics,Pharmacokinetic-modelling,Pharmacokinetics,Population-pharmacokinetics,review},
mendeley-tags = {review},
