file = {:C$\backslash$:/Users/UNIV/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/McMahon et al. - 1990 - Pharmacokinetics and effects of recombinant human erythropoietin after intravenous and subcutaneous injections i.pdf:pdf},
issn = {00064971},
journal = {Blood},
number = {9},
pages = {1718--1722},
title = {{Pharmacokinetics and effects of recombinant human erythropoietin after intravenous and subcutaneous injections in healthy volunteers}},
volume = {76},
year = {1990}
}
