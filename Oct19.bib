file = {:C$\backslash$:/Users/UNIV/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Klein et al. - 2017 - Cergutuzumab amunaleukin (CEA-IL2v), a CEA-targeted IL-2 variant-based immunocytokine for combination cancer immun.pdf:pdf},
issn = {2162402X},
journal = {OncoImmunology},
