issn = {03065251},
journal = {British Journal of Clinical Pharmacology},
keywords = {Bioavailability,ELISA,Intravenous,Pharmacokinetics,Recombinant human interleukin- 11,Subcutaneous},
number = {6},
pages = {571--578},
title = {{Pharmacokinetics of recombinant human interleukin-11 (rhIL-11) in healthy male subjects}},
